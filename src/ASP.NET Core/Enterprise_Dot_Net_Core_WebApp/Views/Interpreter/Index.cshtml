@model Tuple<
                IEnumerable<Enterprise_Dot_Net_Core_WebApp.Core.Entities.Enterprise_MVC_Core>,
                IEnumerable<Enterprise_Dot_Net_Core_WebApp.Core.DTOs.DesginPatterns.Interpreter.InterpreterDto>,
                IEnumerable<Enterprise_Dot_Net_Core_WebApp.Core.DTOs.DesginPatterns.Interpreter.InterpreterDto>
